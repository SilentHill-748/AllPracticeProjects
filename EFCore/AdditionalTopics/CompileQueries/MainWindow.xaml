<Window x:Class="CompileQueries.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CompileQueries"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid
            x:Name="grid"
            Margin="0,50,0,0"/>

        <Button
            x:Name="testGetUserByIdBut"
            VerticalAlignment="Top"
            HorizontalAlignment="Left"
            Content="Test GetUserBuId"
            Click="testGetUserByIdBut_Click"/>

        <Button
            x:Name="testGetUsersByCompanyNameBut"
            VerticalAlignment="Top"
            HorizontalAlignment="Left"
            Content="Test GetUsersByCompanyName"
            Margin="100,0,0,0"
            Click="testGetUsersByCompanyNameBut_Click"/>
    </Grid>
</Window>
